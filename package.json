{
  "name": "habla-bot",
  "version": "1.0.1",
  "description": "A discord bot that can translate text and messages between languages using Google's Cloud Translation API",
  "main": "src/index.js",
  "scripts": {
    "start:dev": "nodemon --watch .env --watch src src/",
    "start:prod": "node src",
    "lint": "eslint src/**/*.js test/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js test/**/*.js --max-warnings 5",
    "test": "jest --config jest.json",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --config jest.json",
    "register": "node src/scripts/register.js"
  },
  "author": "GGanon",
  "license": "UNLICENSE",
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "axios": "^0.21.2",
    "discord-api-types": "^0.36.1",
    "discord.js": "^13.8.0",
    "dotenv": "^8.2.0",
    "iso-639-1": "^2.1.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^28.1.1",
    "lint-staged": "^10.5.3",
    "lorem-ipsum": "^2.0.3",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint:fix",
    "*.{js,css,md}": "prettier --write"
  }
}